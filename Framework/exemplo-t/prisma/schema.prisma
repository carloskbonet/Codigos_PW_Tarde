generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model user {
  id  Int @id @unique @default(autoincrement())

  name  String?
  email String  @unique
  username  String  @unique
  password  String

  created_at DateTime @default(now())
  updated_at DateTime @updatedAt
}
